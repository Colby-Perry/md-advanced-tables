{
  "name": "@susisu/mte-kernel",
  "version": "2.1.0",
  "description": "The text editor independent part of markdown-table-editor",
  "keywords": [
    "markdown",
    "table",
    "text",
    "editor",
    "format",
    "formatter"
  ],
  "files": [
    "lib",
    "dist"
  ],
  "main": "dist/mte-kernel.js",
  "module": "dist/mte-kernel.mjs",
  "scripts": {
    "lint": "eslint lib test",
    "fix": "eslint lib test --fix",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/*.ts'",
    "cover": "nyc -r html -r text npm test",
    "build": "rollup -c",
    "doc": "esdoc",
    "prepublishOnly": "run-s lint test build doc",
    "clean": "rimraf .nyc_output coverage dist doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgrosinger/mte-kernel.git"
  },
  "author": "Susisu <susisu2413@yahoo.co.jp> (https://github.com/susisu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tgrosinger/mte-kernel/issues"
  },
  "homepage": "https://github.com/tgrosinger/mte-kernel#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "6.1.0",
    "@types/chai": "4.2.14",
    "@types/expect": "24.3.0",
    "@types/mocha": "8.0.3",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "chai": "4.2.0",
    "esdoc": "^1.1.0",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.19.1",
    "mocha": "8.2.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^11.7.1",
    "rimraf": "^2.6.2",
    "rollup": "2.33.0",
    "ts-migrate": "0.1.9",
    "ts-migrate-plugins": "0.1.9",
    "ts-node": "9.0.0",
    "tslib": "2.0.3",
    "typescript": "4.0.5"
  },
  "dependencies": {
    "meaw": "^4.3.0"
  }
}
